// Online C compiler to run C program online
#include <stdio.h>

int main() {
    // Write C code here
    int n;
    
printf("enter the elements of an array");
scanf("%d", &n);
int arr[n];
for(int i=0;i<n;i++)
{
    printf("elements %d",i+1);
    scanf("%d" ,&arr[i]);
    
}
int max=arr[0];
int min=arr[0];
for(int i=1;i<n;i++)
{
    printf("%d", arr[i]);
    if(arr[i]>max)
    {

        max=arr[i];
    }
    else{
    
    min=arr[i];
        }
}
        printf("\n Maximum element=%d\n",max);
        printf("\n Minimum element=%d\n", min);
    

    return 0;
}
