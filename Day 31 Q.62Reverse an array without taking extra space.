#include <stdio.h>

int main() {
    // Write C code here
    int n;
    int target=0;
    int found=0;
    int i;
printf("enter the elements of an array  ");
scanf("%d", &n);
int arr[n];
for(int i=0;i<n;i++)
{
    printf("elements %d",i+1);
    scanf("%d" , &arr[i]);
    
}
printf("original array\n");
for(i=0;i<n;i++)
{
    printf("%d\n",arr[i]);
}
printf("The reversed array is\n");
for(i=n-1;i>=0;i--)
{
    printf("%d\n", arr[i]);
}


    return 0;
}
    
    
