#include <stdio.h>

int main() {
    // Write C code here
    int n;
    int sum=0;
printf("enter the elements of an array");
scanf("%d", &n);
int arr[n];
for(int i=0;i<n;i++)
{
    printf("elements %d",i+1);
    scanf("%d" ,&arr[i]);
    sum=sum+arr[i];
    
}
printf("\n th sum of array elements %d", sum);

    return 0;
}
    
    
