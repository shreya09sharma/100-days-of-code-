#include <stdio.h>

int main() {
    int a, b, originalA, originalB, hcf, lcm, temp;

    printf("Enter two positive integers: ");
    scanf("%d %d", &a, &b);

    originalA = a;
    originalB = b;

    // Find HCF using Euclidean Algorithm
    while (b != 0) {
        temp = b;
        b = a % b;
        a = temp;
    }

    hcf = a;

    // Calculate LCM
    lcm = (originalA * originalB) / hcf;

    printf("LCM of %d and %d is: %d\n", originalA, originalB, lcm);

    return 0;
}
