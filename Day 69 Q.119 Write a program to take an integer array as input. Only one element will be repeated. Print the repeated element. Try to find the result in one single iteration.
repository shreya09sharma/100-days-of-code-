#include <stdio.h>

int main() {
    int n;
    printf("Enter the size of the array: ");
    scanf("%d", &n);
    int arr[n];
    for(int i = 0; i < n; i++) {
        printf("Element %d = ", i+1);
        scanf("%d", &arr[i]);
    }
    int f=0;
    int a;
for(int i=0;i<n-1;i++)
{
if(arr[i]==arr[i+1])
{
    f=0;
    break;
}
else
{
    f=1;
    a=arr[i+1];
    break;
    
}
}
printf("%d", a);
    return 0;
}
