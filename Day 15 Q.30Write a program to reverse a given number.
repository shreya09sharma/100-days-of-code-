#include <stdio.h>

int main() {
     int num;
     printf("enter the number\n");
     scanf("%d", &num);
     int temp;
     int rev=0;
     while(num>0)
     {
         int d=num%10;
         rev=rev*10+d;
         num=num/10;
     }

         printf("the reversed number %d\n", rev);
    return 0;
}
