#include <stdio.h>
int main()
{
    int i;
    int n;
  int  numerator=2;
   int denominator=3;
    double sum=0.0;
    
    
    
printf("enter a number of terms\n");
scanf("%d",&n);
for(i=0;i<=n;i++)
{
    sum=sum+(double)numerator/denominator;
    numerator=numerator+2;
    denominator=denominator+4;
}
    printf("the series is%2lf\n", sum);

    return 0;
}
