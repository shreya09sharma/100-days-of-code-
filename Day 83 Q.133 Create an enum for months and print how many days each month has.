#include <stdio.h>

enum Month {
    JANUARY, FEBRUARY, MARCH, APRIL,
    MAY, JUNE, JULY, AUGUST,
    SEPTEMBER, OCTOBER, NOVEMBER, DECEMBER
};

int main() {
    int days[] = {
        31, // January
        28, // February (not considering leap year)
        31, // March
        30, // April
        31, // May
        30, // June
        31, // July
        31, // August
        30, // September
        31, // October
        30, // November
        31  // December
    };

    char *monthNames[] = {
        "January", "February", "March", "April",
        "May", "June", "July", "August",
        "September", "October", "November", "December"
    };

    for (enum Month m = JANUARY; m <= DECEMBER; m++) {
        printf("%s has %d days\n", monthNames[m], days[m]);
    }

    return 0;
}
