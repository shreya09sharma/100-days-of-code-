#include <stdio.h>

int main() {
    int n,found;
    printf("Enter the no. of elements..");
    scanf("%d",&n);
    printf("Enter the elements..");
    int arr[n];
    for (int i=0;i<n;i++)
    {
        scanf("%d",&arr[i]);
    }
    for(int i=0;i<=n;i++)
    {
        found=0;
        for (int j=0;j<n;j++)
        {
            if(arr[j]==i)
            {
                found=1;
            }
        }
        if(found==0)
        {
            printf("The missing element is %d",i);
            break;
        }
    }

    return 0;
}
