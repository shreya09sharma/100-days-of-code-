// Online C compiler to run C program online
#include <stdio.h>
int main() {
    int r,c;
    int sum=0;
    int j;
printf("enter the number of rows\n");
scanf("%d", &r);
printf("enter the number of coloumns\n");
scanf("%d", &c);
int arr[r][c];
for(int i=0;i<r;i++)
{
    for(int j=0;j<c;j++)
    {
    printf("elements [%d][%d]=",i,j);
    scanf("%d",&arr[i][j]);
    sum=sum+arr[i][j];
    }
    
}
    printf("the sum of all elements in a matrix is %d\n", sum);


    return 0;
