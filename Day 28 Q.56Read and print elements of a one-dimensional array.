#include <stdio.h>

int main() {
    int n;
printf("enter the elements of an array");
scanf("%d", &n);
int arr[n];
for(int i=0;i<n;i++)
{
    printf("elements %d",i+1);
    scanf("%d  ",&arr[i]);
    
}
printf("\n the elements of array are\n");
for(int i=0;i<n;i++)
{
    printf("%d",arr[i]);
}

    return 0;
}
