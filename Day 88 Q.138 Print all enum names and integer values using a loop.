#include <stdio.h>

enum Color {
    RED,
    GREEN,
    BLUE,
    YELLOW
};

int main() {
    
    const char *ColorNames[] = {
        "RED",
        "GREEN",
        "BLUE",
        "YELLOW"
    };

    int enumSize = sizeof(ColorNames) / sizeof(ColorNames[0]);

    for (int i = 0; i < enumSize; i++) {
        printf("%s = %d\n", ColorNames[i], i);
    }

    return 0;
}
