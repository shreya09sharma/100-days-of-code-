#include <stdio.h>

int main() {
    // Write C code here
    int num;

    int sum=0;
    int temp;
    
    printf("enter a number\n");
    scanf("%d", &num);
    temp=num;
    while(num>0)
    {  
        int fact=1;
        int d=num%10;
        if(d==0)
        {
        fact=1;
        }
        else
        {
            for(int i=1;i<=d;i++)
            {
                fact=fact*i;
            }
        }
        sum=sum+fact;
        num=num/10;
        
    }
    if(sum==temp)
    {
    printf("strong number");
    }
    else
{
    printf("not a strong number");
}
     
    return 0;
}
