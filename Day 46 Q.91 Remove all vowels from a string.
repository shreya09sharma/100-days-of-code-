#include <stdio.h>
#include <string.h>

int main() {
    char str[100];
    char c;
    char ch;
    int j=0;

    printf("Enter a string\n");
    scanf("%[^\n]", str);

    int len = strlen(str);
    for (int i = 0; i < len; i++) 
    {
        ch=str[i];
        if(!(ch=='a'|| ch=='e' ||ch=='i'||ch=='o'||ch=='u'||ch=='A' || ch=='E'||ch=='I' || ch=='O' || ch=='U'))
        {
            str[j]=ch;
            j++;
        }
    }
    str[j]='\0';
        printf("String without vowels: %s\n", str);

    return 0;
}
