// Online C compiler to run C program online
#include <stdio.h>

int main() {
    int n;
    printf("enter the size of an array\n");
    scanf("%d", &n);
    int arr[n];
    for(int i=0;i<n;i++)
    {
        printf("elements %d=", i+1);
        scanf("%d", &arr[i]);
    }
    int m=-1;
    for (int i=0;i<n;i++)
    {
        int c=0;
        for(int j=0;j<n;j++)
        {
            if(arr[i]==arr[j])
            {
            c++;
            }
        }
    if(c>n/2)
    {
        m=arr[i];
        break;
    }
    }
        printf("%d\n", m);
        
    return 0;
}
