// Online C compiler to run C program online
#include <stdio.h>

int main() {
    int n;
    int temp;
    int k;
    printf("enter the size of an array\n");
    scanf("%d", &n);
    int arr[n];
    printf("enter array elements\n");
    for(int i=0;i<n;i++)
    {
        scanf("%d", &arr[i]);
    }
    printf("Enter the number of positions to rotate right (k):\n");
    scanf("%d", &k);

    k = k % n;
for (int r = 0; r < k; r++)
{
        int temp = arr[n - 1];  

    for(int i=n-1;i>0;i--)
    {
        arr[i]=arr[i-1];
    }
    arr[0]=temp;
}
    for(int i=0;i<n;i++)
    {
        printf("%d", arr[i]);
    }

    return 0;
}
