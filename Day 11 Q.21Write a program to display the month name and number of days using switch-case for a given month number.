#include <stdio.h>

int main() {
    int num;
    printf("enter a number");
    scanf("%d",&num);
    switch(num)
    {
        case 1:
        printf("JANUARY , 31");
        break;
        case 2:
        printf("FEBRUARY , 28");
        break;
        case 3:
        printf("MARCH , 31");
        break;
        case 4:
        printf("APRIL , 30");
        break;
        case 5:
        printf("MAY , 31");
        break;
        case 6:
        printf("JUNE , 30");
        break;
        case 7:
        printf("JULY , 31");
        break;
        case 8:
        printf("August , 31");
        break;
        case 9:
        printf("September , 30");
        break;
        case 10:
        printf("October , 31");
        break;
        case 11:
        printf("November , 30");
        break;
        case 12:
        printf("december , 31");
        break;
        default:
        printf("WRONG INPUT");
    }

    return 0;
}
