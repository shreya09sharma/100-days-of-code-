#include <stdio.h>

int main() {
    int n;
    printf("Enter the number of elements of a sorted array:\n");
    scanf("%d", &n);

    int arr[n];
    for(int i = 0; i < n; i++) {
        printf("Element %d = ", i + 1);
        scanf("%d", &arr[i]);
    }

    int x;
    printf("Enter value of x:\n");
    scanf("%d", &x);

    int ceilIndex = -1;

  
    for(int i = 0; i < n; i++) {
        if(arr[i] >= x) {
            ceilIndex = i;
            break; 
        }
    }

    printf("%d\n", ceilIndex);

    return 0;
}
