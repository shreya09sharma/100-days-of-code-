// Online C compiler to run C program online
#include <stdio.h>

int main() {
    // Write C code here
    int n;
    int pcount=0;
    int ncount=0;
    int zcount=0;
printf("enter the elements of an array");
scanf("%d", &n);
int arr[n];
for(int i=0;i<n;i++)
{
    printf("elements %d",i+1);
    scanf("%d" ,&arr[i]);
    
}

for(int i=0;i<n;i++)
{
    printf("%d", arr[i]);
    if(arr[i]>0)
    {

        pcount++;
    }
    else if(arr[i]<0)
    {
    
    ncount++;
        }
        else
        {
            zcount++;
        }
}
        printf("\n positive numbers=%d\n",pcount);
        printf("\n negative number=%d\n", ncount);
        printf("\n zero=%d\n",zcount);
    

    return 0;
}
    
