#include <stdio.h>

int main() {
     int num;
     int sum=0;
     printf("enter the number\n");
     scanf("%d", &num);
     int temp;
     temp=num;
     while(num>0)
     {
         int d=num%10;
         sum=sum+(d*d*d);
         num=num/10;
         
     }
     if(sum==temp)
     {
         printf("the number is armstrong");
         
     }
     else
     {
         printf("the number is not armstrong");
     }
         
    return 0;
}
