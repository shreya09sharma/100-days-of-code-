#include <stdio.h>

int main() {
    int n1,n2;
    int sum,diff,product,quotient;
    printf("enter two numbers n1 and n2 \n");
    scanf("%d %d",&n1,&n2);
    sum=n1+n2;
    printf("Sum=%d\n",sum);
    diff=n1-n2;
    printf("diff=%d \n",diff);
    product=n1*n2;
    printf("product=%d \n",product);
    quotient=n1/n2;
    printf("quotient=%d \n",quotient);

    return 0;
}
