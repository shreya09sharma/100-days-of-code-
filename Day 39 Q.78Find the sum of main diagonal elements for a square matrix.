#include <stdio.h>

int main() {
    int matrix[10][10];  // assuming max 10x10 matrix
    int n, i, j;
    int sum = 0;

    
    printf("Enter the size of square matrix (max 10): ");
    scanf("%d", &n);

    
    printf("Enter the elements of the matrix:\n");
    for (i = 0; i < n; i++) {
        for (j = 0; j < n; j++) {
            scanf("%d", &matrix[i][j]);
        }
    }

    
    for (i = 0; i < n; i++)
    {
        sum += matrix[i][i];  // only elements where row == column
    }

    
    printf("Sum of main diagonal elements = %d\n", sum);

    return 0;
}
