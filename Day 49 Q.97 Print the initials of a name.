
#include <stdio.h>

int main() {
    // Write C code here
    char str[100];
    printf("Enter a string\n");
    scanf("%[^\n]", str);
    char ch;
    for(int i =0;str[i]!='\0';i++)
    {
        ch=str[i];
    if(ch==' ')
    {
        printf("%c.%c\n",str[0],str[i+1]);
    }
    }
    return 0;
}
