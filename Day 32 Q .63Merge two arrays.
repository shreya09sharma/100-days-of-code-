// Online C compiler to run C program online
#include <stdio.h>

int main() {
    // Write C code here
    int n;
    int i;
    int h;
printf("enter the elements of an array  ");
scanf("%d", &n);
int arr[n];

for(int i=0;i<n;i++)
{
    printf("element %d =",i+1);
    scanf("%d" , &arr[i]);
    
}
printf("First array\n");

for(i=0;i<n;i++)
{
    printf("%d\n",arr[i]);
}
int s;
printf(" enter the size of an array \n");
scanf("%d",&s);
int array[s];
for( int h=0;h<s;h++)
{
    printf("element %d =",h+1);
    scanf("%d", &array[h]);
}
printf("second array\n");
for(int h=0;h<s;h++)
{
    printf("%d\n",array[h]);
}
printf("The merged  array is\n");
int merged[n + s];
for(int i=0;i<n;i++)
{
    merged[i]=arr[i];
}
for(int h=0;h<s;h++)
{
    merged[n+h]=array[h];
    
}
printf("\nThe merged array is:\n");
    for(int i = 0; i < n + s; i++) {
        printf("%d\n", merged[i]);
    }
    return 0;
}
    
